#include<stdio.h>
int main()
{
	int n,i;
	int arr[100];
	printf("enter no.of elements:");
	scanf("%d",&n);
	for( i=0;i<n;++i)
	{
	printf("enter number%d:",i+1);
	scanf("%d",&arr[i]);
}
 for (int i=1;i<n;++i) {
if (arr[0]<arr[i]) {
   arr[0]=arr[i];
}
}
printf("largest number=%d",arr[0]);
return 0;
}
