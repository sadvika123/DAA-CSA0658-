#include<stdio.h>
#define max_size 50
int main()
{
	int arr[max_size];
	int i,max,min,s;
	printf("enter size of the array:");
	scanf("%d",&s);
	printf("enter elements in the array:");
	for(i=0;i<s;i++)
	{
		scanf("%d",&arr[i]);
	}
	max=arr[0];
	min=arr[0];
	for(i=1;i<s;i++)
	{
		if(arr[i]>max)
		{
			max=arr[i];
		}
		if(arr[i]<min)
		{
			min=arr[i];
		}
	}
	printf("maximun element=%d\n",max);
	printf("minimum element=%d\n",min);
	return 0;
	
}
